{
  "name": "@odoo/o-spreadsheet",
  "version": "19.1.0-alpha.3",
  "description": "A spreadsheet component",
  "type": "module",
  "scripts": {
    "build": "npm-run-all build:js bundle:iife && npm run copy:xml:build && npm run copy:css:build",
    "build:js": "tsc --module es6 --incremental",
    "bundle:esm": "rollup -c -m -- --format esm",
    "bundle:iife": "rollup -c -m -- --format iife",
    "dist": "tsc --module es6 --declaration --declarationDir dist/types && rollup -c && npm run copy:xml:dist && npm run copy:css:dist",
    "copy:xml:dist": "cpy ../o-spreadsheet-ui/dist/o_spreadsheet.xml dist/",
    "copy:css:dist": "cpy ../o-spreadsheet-ui/dist/o_spreadsheet.css dist/",
    "copy:xml:build": "cpy ../o-spreadsheet-ui/build/o_spreadsheet.xml build/",
    "copy:css:build": "cpy ../o-spreadsheet-ui/build/o_spreadsheet.css build/"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^2.2.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@swc/core": "1.6.7",
    "@swc/jest": "0.2.36",
    "@types/jest": "^27.0.1",
    "@types/node": "^20.17.24",
    "@types/rbush": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.19.0",
    "chart.js": "4.4.5",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-chart-geo": "^4.3.2",
    "cors": "^2.8.5",
    "cpy-cli": "^6.0.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.19",
    "express-ws": "^4.0.0",
    "file-saver": "^2.0.5",
    "fs": "^0.0.1-security",
    "glob": "^11.0.1",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jszip": "^3.6.0",
    "lint-staged": "^12.1.2",
    "live-server": "^1.2.1",
    "luxon": "^3.5.0",
    "minimist": "^1.2.8",
    "mockdate": "^3.0.2",
    "node-watch": "^0.7.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "seedrandom": "^3.0.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.30.1",
    "xml-formatter": "^2.4.0"
  },
  "dependencies": {
    "@odoo/o-spreadsheet-engine": "*",
    "@odoo/o-spreadsheet-ui": "*"
  }
}
