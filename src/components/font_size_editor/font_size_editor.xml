<templates>
  <t t-name="o-spreadsheet-FontSizeEditor">
    <NumberEditor
      currentValue="props.currentFontSize"
      onValueChange="props.onFontSizeChanged"
      class="props.class"
      onToggle="props.onToggle"
      onFocusInput="props.onFocusInput"
      min="1"
      max="400"
      valueList="fontSizes"
      title.translate="Font Size"
    />
  </t>
</templates>
