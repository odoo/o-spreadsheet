<templates>
  <t t-name="o-spreadsheet-AddDimensionButton">
    <AutocompletePopover values="values" onValuePicked.bind="onValuePicked">
      <button class="add-dimension o-button">Add</button>
      <t t-set-slot="popoverContent">
        <t t-slot="default"/>
      </t>
    </AutocompletePopover>
  </t>
</templates>
