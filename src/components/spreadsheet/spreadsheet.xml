<templates>
  <t t-name="o-spreadsheet-Spreadsheet">
    <div class="o-spreadsheet h-100 w-100" t-att-class="{'o-spreadsheet-mobile': env.isSmall}">
      <t t-if="fullScreenSheetStore.fullScreenSheetId">
        <FullScreenSheet/>
      </t>
      <t t-elif="env.isDashboard()">
        <SpreadsheetDashboard/>
        <FullScreenChart/>
      </t>
      <t t-else="">
        <SpreadsheetEditor/>
      </t>
    </div>
  </t>
</templates>
